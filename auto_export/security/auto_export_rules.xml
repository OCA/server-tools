<odoo>

    <record model="ir.rule" id="auto_export_manager_rule">
        <field name="name">auto.export.manager.rule</field>
        <field name="model_id" ref="auto_export.model_auto_export"/>
        <field name="domain_force">[('user_id', '=', user.id)]</field>
        <field name="perm_read" eval="0"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_unlink" eval="1"/>
        <field name="groups" eval="[(6, 0, [ref('auto_export.auto_export_manager')])]"/>
    </record>

</odoo>
