<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!--SETTINGS-->
        <record id="default_auth_password_min_character" model="ir.config_parameter">
            <field name="key">auth_password_settings.auth_password_min_character</field>
            <field name="value">6</field>
        </record>
        <record id="default_auth_password_has_capital_letter" model="ir.config_parameter">
            <field name="key">auth_password_settings.auth_password_has_capital_letter</field>
            <field name="value">False</field>
        </record>
        <record id="default_auth_password_has_digit" model="ir.config_parameter">
            <field name="key">auth_password_settings.auth_password_has_digit</field>
            <field name="value">False</field>
        </record>
        <record id="default_auth_password_has_special_letter" model="ir.config_parameter">
            <field name="key">auth_password_settings.auth_password_has_special_letter</field>
            <field name="value">False</field>
        </record>
    </data>
</openerp>
