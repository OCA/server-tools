<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="video_provider_youtube" model="video.provider">
        <field name="code">youtube</field>
        <field name="name">Youtube</field>
        <field
            name="pattern_video_url"
        >https://www.youtube.com/watch?v={record.identifier}</field>
        <field
            name="pattern_thumbnail_url"
        >https://img.youtube.com/vi/{record.identifier}/0.jpg</field>
    </record>

</odoo>
