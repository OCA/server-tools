<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

        <record id="event_to_plan" model="calendar.event.state">
            <field name="name">To Plan</field>
            <field name="sequence" eval="1"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="event_planned" model="calendar.event.state">
            <field name="name">Planned</field>
            <field name="sequence" eval="2"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="event_done" model="calendar.event.state">
            <field name="name">Done</field>
            <field name="sequence" eval="3"/>
            <field name="fold" eval="True"/>
        </record>

    </data>
</odoo>
