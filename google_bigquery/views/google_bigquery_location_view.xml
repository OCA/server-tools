<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="google_bigquery_location_tree" model="ir.ui.view">
        <field name="model">google.bigquery.location</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name" />
                <field name="description" />
            </tree>
        </field>
    </record>

    <record id="google_bigquery_location_action" model="ir.actions.act_window">
        <field name="name">Locations</field>
        <field name="res_model">google.bigquery.location</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
