<openerp>
    <data>
        <record id="ae_password_prompt_wiz_view" model="ir.ui.view">
            <field name="name">ae.password.prompt.wizard.view</field>
            <field name="model">ae.password.prompt.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <h3>
                            <field name="name" nolabel="1" />
                        </h3>
                        <group>
                            <field name="password" password="1" required="1" />
                        </group>
                        <separator colspan="4" />
                        <group>
                            <button class="oe_highlight" type="object" string="Continue" name="do_next_action" colspan="1" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>
