#. module: sql_export_mail
#: model:mail.template,subject:sql_export_mail.sql_export_mailer
msgid "${object.name or ''}"
msgstr ""

#. module: sql_export_mail
#: model:mail.template,body_html:sql_export_mail.sql_export_mailer
msgid "<div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"<p>You will find the report ${object.name or ''} as an attachment of the mail.</p>\n"
"% if not object.mail_active:\n"
"<p>Mail sending has been muted.\n"
"When you have dealt with the subject, please reactivate the alert\n"
"by clicking on <a href=\"${object.get_url()}\">this link.</a>\n"
"</p>\n"
"% endif\n"
"</div>\n"
"        "
msgstr "<div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"<p>Vous trouverez le rapport ${object.name or ''} en pièce jointe.</p>\n"
"% if not object.mail_active:\n"
"<p>L'envoie de mail a été désactivé.\n"
"quand vous avez traité votre alerte, veuillez la réactiver en cliquant sur\n"
"by clicking on <a href=\"${object.get_url()}\">ce lien.</a>\n"
"</p>\n"
"% endif\n"
"</div>\n"
"        "

